{
    "name": "int_algolia_sfra",
    "version": "23.1.0",
    "description": "Algolia SFRA Link Cartridge",
    "scripts": {
        "test": "sgmf-scripts --test test/unit/**/*.js",
        "test:integration": "sgmf-scripts --integration 'test/integration/**/*.js'",
        "copy-scripts": "copy-files-from-to",
        "compile:scss": "sgmf-scripts --compile css",
        "compile:js": "sgmf-scripts --compile js",
        "compile:fonts": "node bin/Makefile compileFonts",
        "lint:js": "sgmf-scripts --lint js",
        "upload": "sgmf-scripts --upload",
        "uploadCartridge": "sgmf-scripts --uploadCartridge int_algolia && sgmf-scripts --uploadCartridge int_algolia_controllers && sgmf-scripts --uploadCartridge int_algolia_sfra && sgmf-scripts --uploadCartridge bm_algolia",
        "watch": "sgmf-scripts --watch",
        "watch:static": "sgmf-scripts --watch static"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/algolia/algoliasearch-sfcc-b2c.git"
    },
    "engines": {
        "node": ">=14"
    },
    "devDependencies": {
        "copy-files-from-to": "^3.8.0",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.5",
        "eslint-plugin-jquery": "^1.5.1",
        "eslint-plugin-prettier": "^4.2",
        "prettier": "^2.7",
        "proxyquire": "2.1.3",
        "sgmf-scripts": "^2.4.2",
        "@algolia/autocomplete-theme-classic": "1.8.3",
        "algoliasearch": "3.35.1",
        "hogan.js": "1.0.2",
        "instantsearch.css": "7.4.5",
        "instantsearch.js": "4.52.0",
        "search-insights": "1.3.1"
    },
    "browserslist": [
        "last 2 versions",
        "ie >= 10"
    ],
    "babel": {
        "presets": [
            "env"
        ]
    },
    "packageName": "int_algolia_sfra",
    "paths": {
        "base": "../storefront-reference-architecture/cartridges/app_storefront_base/"
    }
}
